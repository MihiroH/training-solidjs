{
  "name": "training-solidjs",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "run-p lint:style lint:script",
    "lint:fix": "run-p lint:style:fix lint:script:fix",
    "lint:script": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "lint:script:fix": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint src/**/*.scss",
    "lint:style:fix": "stylelint --fix src/**/*.scss",
    "format": "prettier --write --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx,json,scss}'",
    "convert:png2webp": "npx @squoosh/cli -d public/img --webp auto public/img/*.png",
    "prepare": "husky install",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@squoosh/lib": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-solid": "^0.7.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vite-plugin-fonts": "^0.6.0",
    "vite-plugin-solid": "^2.3.0"
  },
  "dependencies": {
    "@solidjs/router": "^0.4.3",
    "autoprefixer": "^10.4.8",
    "sass": "^1.54.8",
    "solid-js": "^1.5.1"
  }
}
